<refentry xml:id="zbarcodeimage.clear" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ZBarCodeImage::clear</refname>
  <refpurpose>Clear the pages</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>ZBarCodeImage</type><methodname>ZBarCodeImage::clear</methodname>
   <void />
  </methodsynopsis>
  <para>
   Removes all pages from the ZBarCodeImage object.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the current object on success and throws ZBarCodeException on failure.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ZBarCodeScanner::scan</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

